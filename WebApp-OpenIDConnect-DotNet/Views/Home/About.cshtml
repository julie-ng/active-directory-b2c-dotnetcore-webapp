@{
    ViewData["Title"] = "About";

}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>

@using System.Security.Claims

<table class="table table-striped">
    <tr>
        <th>Claim Type</th>
        <th>Claim Value</th>
    </tr>

    @foreach (Claim claim in User.Claims)
    {
        <tr>
            <td>@claim.Type</td>
            <td><code>@claim.Value</code></td>
        </tr>
    }
</table>
